{
	"name": "obsidian-incremental-reading",
	"version": "1.0.0",
	"description": "SuperMemo-inspired incremental reading with extracts, cloze deletions, and spaced repetition.",
	"author": "Johan Dahlin",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"typecheck": "tsc -p tsconfig.build.json -noEmit -skipLibCheck && tsc -p tests/tsconfig.json -noEmit -skipLibCheck",
		"build": "tsc -p tsconfig.build.json -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"test": "vitest run --coverage",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"prepare": "husky"
	},
	"keywords": [
		"obsidian",
		"incremental-reading",
		"spaced-repetition",
		"flashcards",
		"cloze",
		"learning"
	],
	"license": "MIT",
	"devDependencies": {
		"@codemirror/state": "6.5.0",
		"@codemirror/view": "^6.29.1",
		"@eslint/js": "9.30.1",
		"@types/node": "^18.0.0",
		"@vitest/coverage-v8": "^1.6.0",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"husky": "^9.1.7",
		"jiti": "2.6.1",
		"prettier": "^3.7.4",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"vitest": "^1.6.0",
		"yaml": "^2.6.0"
	},
	"dependencies": {
		"obsidian": "latest",
		"preact": "^10.26.9",
		"ts-fsrs": "^5.2.3"
	}
}
