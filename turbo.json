{
	"$schema": "https://turbo.build/schema.json",
	"ui": "stream",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [],
			"outputLogs": "errors-only"
		},
		"@repo/obsidian#build": {
			"dependsOn": ["^build"],
			"outputs": ["main.js", "styles.css"],
			"outputLogs": "errors-only"
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"lint": {
			"dependsOn": ["^build"],
			"outputs": [],
			"outputLogs": "errors-only"
		},
		"test": {
			"dependsOn": ["^build"],
			"outputLogs": "errors-only"
		},
		"test:coverage": {
			"dependsOn": ["^build"],
			"outputLogs": "errors-only"
		},
		"test:watch": {
			"cache": false,
			"persistent": true
		},
		"typecheck": {
			"dependsOn": ["^build"]
		},
		"clean": {
			"cache": false
		}
	}
}
