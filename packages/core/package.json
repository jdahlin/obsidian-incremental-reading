{
	"name": "@repo/core",
	"type": "module",
	"version": "0.0.0",
	"private": true,
	"exports": {
		".": "./src/index.ts",
		"./*": "./src/*"
	},
	"main": "src/index.ts",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"lint": "eslint .",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"better-sqlite3": "catalog:",
		"js-yaml": "catalog:",
		"ts-fsrs": "^5.2.3",
		"yaml": "catalog:"
	},
	"devDependencies": {
		"@repo/eslint-config": "workspace:*",
		"@repo/tsconfig": "workspace:*",
		"@repo/vitest-config": "workspace:*",
		"@types/better-sqlite3": "catalog:",
		"@types/js-yaml": "catalog:",
		"@types/node": "catalog:",
		"typescript": "catalog:",
		"vitest": "catalog:"
	}
}
